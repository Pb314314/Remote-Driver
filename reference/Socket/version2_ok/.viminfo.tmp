	#!/usr/bin/env python2
	
	import rospy
	from geometry_msgs.msg import Twist, Vector3
	import socket
	
	def receiveOperation(sock):
	    data = sock.recv(16)
	    l, a = [float(e) for e in data.split()]
	    lv = Vector3(x=l, y=0, z=0)
	    av = Vector3(x=0, y=0, z=a)
	    twist = Twist(linear=lv, angular=av)
	    return twist
	
	if __name__ == '__main__':
	    rospy.init_node('op_receiver')
	    lsock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
	    lsock.bind(('10.107.84.22', 2020))
	    lsock.listen(1)
	    conn_sock, _ = lsock.accept()
	    lsock.close()
	
	    pub = rospy.Publisher('cmd_vel', Twist, queue_size=16)
	
	    try:
	        while not rospy.is_shutdown():
	            twist = receiveOperation(conn_sock)
	            pub.publish(twist)
	
	    except:
	        pass
	
	    finally:
	        conn_sock.close()
	        lv = Vector3(x=0, y=0, z=0)
	        av = Vector3(x=0, y=0, z=0)
	        twist = Twist(linear=lv, angular=av)
	        pub.publish(twist)
	        # rospy.on_shutdown(myhook)
